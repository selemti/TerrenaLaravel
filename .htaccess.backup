<IfModule mod_rewrite.c>
    RewriteEngine On

    # Redirect everything to public folder except if already there or actual files/folders exist
    RewriteCond %{REQUEST_URI} !^/TerrenaLaravel/public/
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule ^(.*)$ public/$1 [L,QSA]
</IfModule>
